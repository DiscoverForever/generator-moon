{
  "name": "generator-moon",
  "version": "0.0.0",
  "description": "Generator vue&leancloud project",
  "homepage": "https://discoverforever.github.io",
  "author": {
    "name": "tangyy",
    "email": "yueyangtang@163.com",
    "url": "https://github.com/DiscoverForever"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "vue",
    "leancloud",
    "javascript",
    "yeoman-generator"
  ],
  "devDependencies": {
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-xo": "^0.20.1",
    "eslint-plugin-prettier": "^2.6.0",
    "husky": "^0.14.3",
    "jest": "^22.0.6",
    "lint-staged": "^6.1.1",
    "nsp": "^2.8.0",
    "prettier": "^1.11.1",
    "yeoman-assert": "^3.1.0",
    "yeoman-test": "^1.7.0"
  },
  "engines": {
    "npm": ">= 4.0.0"
  },
  "dependencies": {
    "chalk": "^2.1.0",
    "deepmerge": "^2.1.1",
    "jhipster-core": "^3.0.2",
    "loadsh": "^1.0.1",
    "querystring": "^0.2.0",
    "xml2js": "^0.4.19",
    "yeoman-generator": "^2.0.1",
    "yosay": "^2.0.1"
  },
  "jest": {
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "nsp check",
    "pretest": "eslint . --fix",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "license": "MIT"
}
